{
  "extends": ["airbnb", "prettier", "plugin:react/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "no-case-declarations": 0,
    "react/no-unescaped-entities": 0,
    "react/prop-types": 0,
    "react/jsx-one-expression-per-line": 0,
    "react/jsx-filename-extension": 0,
    "no-unused-vars": 0
  }
}
